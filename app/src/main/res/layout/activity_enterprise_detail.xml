<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/view">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="企业详情"
            android:gravity="center"
            android:padding="10dp"
            android:id="@+id/tv_title" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mygsnametext"
                    android:padding="10dp"
                    android:id="@+id/textView4" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_gsname"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView4"
                    android:layout_toEndOf="@+id/textView4" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_gsname"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view3" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/zhanweihaotext"
                    android:padding="10dp"
                    android:id="@+id/textView5" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_zhanweihao"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView5"
                    android:layout_toEndOf="@+id/textView5" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_zhanweihao"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view4" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gspropertytext"
                    android:padding="10dp"
                    android:id="@+id/textView6" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_gsproperty"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView6"
                    android:layout_toEndOf="@+id/textView6" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_gsproperty"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view6" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gsareatext"
                    android:padding="10dp"
                    android:id="@+id/textView7" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_gsarea"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView7"
                    android:layout_toEndOf="@+id/textView7" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_gsarea"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view7" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gsprofiletext"
                    android:padding="10dp"
                    android:id="@+id/textView8" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_gsprofile"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView8"
                    android:layout_toEndOf="@+id/textView8" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_gsprofile"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view8" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/phonenumtext"
                    android:padding="10dp"
                    android:id="@+id/textView9" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_phonenum"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView9"
                    android:layout_toEndOf="@+id/textView9" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_phonenum"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view9" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/exhibitstext"
                    android:padding="10dp"
                    android:id="@+id/textView0" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/tv_exhibits"
                    android:padding="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/textView0"
                    android:layout_toEndOf="@+id/textView0" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/tv_exhibits"
                    android:background="@color/black"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view0" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/exhibitortext"
                    android:padding="10dp"
                    android:id="@+id/textView11" />
                <RelativeLayout
                    android:id="@+id/rv_renyuan1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/img_photo1"
                            android:src="@mipmap/ic_launcher"
                            android:layout_centerVertical="true"
                             />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="New Text"
                            android:id="@+id/tv_name1"
                            android:layout_marginTop="20dp"
                            android:layout_below="@+id/img_photo1"
                            android:layout_alignLeft="@+id/img_photo1"
                            android:layout_alignStart="@+id/img_photo1"
                            android:layout_centerHorizontal="true"
                            />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/rv_renyuan1"
                    android:layout_toEndOf="@+id/rv_renyuan1"
                    android:layout_weight="1">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/img_photo2"
                            android:src="@mipmap/ic_launcher"
                            android:layout_centerVertical="true"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="New Text"
                            android:id="@+id/tv_name2"
                            android:layout_marginTop="20dp"
                            android:layout_below="@+id/img_photo2"
                            android:layout_alignLeft="@+id/img_photo2"
                            android:layout_alignStart="@+id/img_photo2"
                            android:layout_centerHorizontal="true"
                            />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/img_photo2"
                    android:layout_weight="1">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/img_photo3"
                            android:src="@mipmap/ic_launcher"
                            android:layout_centerVertical="true"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="New Text"
                            android:id="@+id/tv_name3"
                            android:layout_marginTop="20dp"
                            android:layout_below="@+id/img_photo3"
                            android:layout_alignLeft="@+id/img_photo3"
                            android:layout_alignStart="@+id/img_photo3"
                            android:layout_centerHorizontal="true"
                            />
                </RelativeLayout>

        </LinearLayout>



</LinearLayout>

